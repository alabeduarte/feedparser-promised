{
  "name": "feedparser-promised",
  "version": "2.0.0-rc",
  "description": "Wrapper around feedparser with promises",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint src/**",
    "format": "prettier-eslint \"src/**/*.js\" --write",
    "pretest": "yarn lint",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "prepare": "babel src -d lib --ignore 'src/*.test.js'",
    "smoke": "./smoke.sh"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "@types/node-feedparser": "2.2.1",
    "@types/request": "2.47.1",
    "feedparser": "2.2.9",
    "request": "2.79.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.3.0",
    "faker": "^4.1.0",
    "jest": "^23.4.2",
    "nock": "10.0.0",
    "prettier-eslint-cli": "^4.7.1",
    "xmlbuilder": "^10.0.0"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alabeduarte/feedparser-promised.git"
  },
  "keywords": [
    "feed",
    "rss",
    "node",
    "nodejs",
    "javascript",
    "parser",
    "promise"
  ],
  "babel": {
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ],
    "presets": [
      "es2015"
    ]
  },
  "author": "Alabe Duarte",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alabeduarte/feedparser-promised/issues"
  },
  "homepage": "https://github.com/alabeduarte/feedparser-promised#readme"
}
